{
  "name": "my-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run compile && npm run lint && npm test",
    "clean:all": "rimraf ./node_modules",
    "clean:install": "npm run clean:all && npm install",
    "clean:obsolete-snapshots": "npm test -- -u",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:clean": "rm -rf lib",
    "compile:build": "babel src --out-dir lib --copy-files --ignore src/site",
    "docs": "npm run docs:generate && npm run docs:reports",
    "docs:generate": "jsdoc --configure jsdoc.config.json --package package.json -d target/doc ./src/*.jsx",
    "docs:reports": "nightwatch-html-reporter -d ./target/reports/nigthwatch -b false -t cover",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .js,.jsx .",
    "prepush": "node scripts/prepush/index.js",
    "pretest": "npm run lint",
    "start": "webpack-dev-server --progress",
    "test": "npm run test:jest && npm run test:nightwatch-default",
    "test:jest": "jest --config jestconfig.json",
    "test:nightwatch-default": "WEBPACK_CONFIG_PATH=$(pwd)/webpack.config.js node ./node_modules/terra-toolkit/lib/scripts/nightwatch-process.js --env default-tiny,default-small,default-medium"
  },
  "dependencies": {
    "airbnb-prop-types": "^2.8.1",
    "axios": "^0.16.2",
    "file-loader": "^0.11.2",
    "pdfjs-dist": "^1.9.448",
    "prop-types": "^15.5.8",
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-intl": "^2.3.0",
    "react-on-rails": "7.0.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3",
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.8.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "enzyme-react-intl": "^1.4.5",
    "enzyme-redux": "^0.1.6",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.0.0",
    "jsdoc": "^3.5.4",
    "json-loader": "^0.5.4",
    "nightwatch-html-reporter": "^2.0.2",
    "node-sass": "^4.5.3",
    "postcss-custom-properties": "^6.0.1",
    "postcss-loader": "^2.0.5",
    "postcss-rtl": "^0.5.10",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-intl": "^2.3.0",
    "react-on-rails": "7.0.4",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3",
    "redux-test-utils": "^0.1.2",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "shelljs": "^0.7.7",
    "style-loader": "^0.17.0",
    "stylelint": "^7.10.1",
    "stylelint-config-sass-guidelines": "^2.1.0",
    "terra-toolkit": "^1.2.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
